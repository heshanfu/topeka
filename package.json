{
  "name": "topeka",
  "version": "0.5.0",
  "description": "Idiomatic, two-way bindings, in React",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "tdd": "karma start",
    "lint": "eslint test src",
    "example": "rimraf ./example/bundle.js && webpack-dev-server --config webpack.examples.babel.js",
    "build:example": "webpack --config webpack.examples.babel.js",
