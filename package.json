{
  "name": "topeka",
  "version": "0.5.0",
  "description": "Idiomatic, two-way bindings, in React",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint test src",
    "docs": "rimraf ./docs/bundle.js && webpack-dev-server --config docs/webpack.config.js",
    "build:docs": "webpack --config docs/webpack.config.js",
    "build": "npm run test && npm run build:docs && babel src --out-dir lib",
    "release": "release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jquense/topeka.git"
  },
  "keywords": [
    "react",
    "binding",
    "input",
    "form",
    "valueLink"
  ],
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/topeka/issues"
  },
  "homepage": "https://github.com/jquense/topeka#readme",
  "devDependencies": {
    "@monastic.panic/component-playground": "^3.0.0",
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-preset-jason": "^3.0.0",
    "chai": "^3.5.0",
    "component-metadata-loader": "^3.0.1",
    "eslint": "^3.19.0",
    "eslint-config-jason": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^20.0.1",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "mocha": "^2.3.3",
    "prop-types": "^15.5.0",
    "raw-loader": "^0.5.1",
    "react": "^15.0.1",
    "react-bootstrap": "^0.29.2",
    "react-component-metadata": "^3.1.0",
    "react-dom": "^15.0.1",
    "react-overlays": "^0.6.3",
    "release-script": "^0.5.3",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "teaspoon": "^6.5.0",
    "webpack": "^2.5.1",
    "webpack-atoms": "^1.0.2",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "react": "^0.14.9 || >=15.3.0"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "classnames": "^2.2.5",
    "invariant": "^2.2.2",
